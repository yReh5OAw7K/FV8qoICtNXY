# Example environment variables for the Academic Research Assistant
# Copy this file to .env and fill in your actual values

# Google ADK API Key (required)
GOOGLE_API_KEY=your_google_api_key_here

# Model to use (required)
MODEL=gemini-2.0-flash

# Web driver configuration (optional)
DISABLE_WEB_DRIVER=0  # 0=enabled, 1=disabled

# SerpAPI Key (optional - used as fallback when Scrapy search fails)
# Get a key from: https://serpapi.com/
SERPAPI_KEY=your_serpapi_key_here 